    "jquery": "^3.5.1",
    "justifiedGallery": "^3.8.1",
    "laravel-mix": "^5.0.9",
    "mdi": "^2.2.43",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "devDependencies": {
    "resolve-url-loader": "3.1.2",
    "sass": "^1.29.0",
    "sass-loader": "10.*"
  }
